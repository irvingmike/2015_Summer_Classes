  </xs:attributeGroup>
  <!-- end of SVG.font-face-name.attlist -->
  <!-- definition-src: Definition Source Element ......... -->
  <xs:element name="definition-src">
    <xs:complexType>
      <xs:attributeGroup ref="svg:attlist.definition-src"/>
    </xs:complexType>
  </xs:element>
